{
  "extends": "airbnb-base",
  "parser": "@babel/eslint-parser",
  "env": {
    "browser": true,
    "jquery": true
  },
  "rules": {
    "no-console": "off",
    "import/no-extraneous-dependencies": ["error", {"devDependencies": ["gulpfile.babel.js", "gulp/**/*", "src/js/**/*.js"]}],
    "import/prefer-default-export": "off",
    "import/extensions": "off",
    "no-unused-vars" : "off",
    "no-param-reassign": 0
  }
}
